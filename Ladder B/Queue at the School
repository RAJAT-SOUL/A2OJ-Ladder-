#include<bits/stdc++.h>
using namespace std;
typedef long long int ll;
 
 
 
int main()
{
	int n;
	int t;
	cin>>n>>t;
	string s;
	cin>>s;
	for(int j=1;j<=t;j++)
	{
		for(int i=1;i<=n;i++)
		{
			if(s[i-1]=='B' && s[i]=='G')
				{
					s[i-1]='G';
					s[i]='B';
					i++;
		        }	
			
		}
		
	}
	cout<<s<<endl;
	
}
